-- [3] Ù…Ø­Ø±Ùƒ Ø§Ù„Ù…Ø´ÙŠ ÙˆØ§Ù„Ø³Ø±Ø¹Ø© 152
local function CreateBtn(txt, pos)
    local btn = Instance.new("TextButton", main)
    btn.Size = UDim2.new(0.85, 0, 0, 35)
    btn.Position = pos
    btn.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
    btn.BackgroundTransparency = 0.92
    btn.Text = txt
    btn.TextColor3 = Color3.new(1, 1, 1)
    btn.Font = Enum.Font.GothamBold
    btn.TextSize = 11
    Instance.new("UICorner", btn).CornerRadius = UDim.new(0, 8)
    return btn
end

local points = {
    Vector3.new(201.03, 0, 2.81), Vector3.new(286.39, 0, 11.53),
    Vector3.new(401.17, 0, 12.12), Vector3.new(546.4, 0, -1.41),
    Vector3.new(759.24, 0, 3.27), Vector3.new(1074.91, 0, 8.26),
    Vector3.new(1554.72, 0, 2.68), Vector3.new(2267.09, 0, 10.27),
    Vector3.new(2611.29, 0, -2.11)
}
local currentIdx = 0
local stationBtn = CreateBtn("Ø§Ù„Ù…Ø­Ø·Ø© Ø§Ù„ØªØ§Ù„ÙŠØ© ðŸƒâ€â™‚ï¸", UDim2.new(0.075, 0, 0, 80))
local isMoving = false
local moveConn = nil

stationBtn.Activated:Connect(function()
    if isMoving then isMoving = false if moveConn then moveConn:Disconnect() end stationBtn.Text = "Ø§Ù„Ù…Ø­Ø·Ø© Ø§Ù„ØªØ§Ù„ÙŠØ© ðŸƒâ€â™‚ï¸" return end
    local char = lp.Character
    local hrp = char and char:FindFirstChild("HumanoidRootPart")
    if hrp then
        isMoving = true
        stationBtn.Text = "Ø¬Ø§Ø±ÙŠ Ø§Ù„Ù…Ø´ÙŠ..."
        currentIdx = (currentIdx % #points) + 1
        local target = points[currentIdx]
        moveConn = rs.Heartbeat:Connect(function()
            if not isMoving or not hrp.Parent then if moveConn then moveConn:Disconnect() end return end
            local currentPos = hrp.Position
            local targetFlat = Vector3.new(target.X, currentPos.Y, target.Z)
            local diff = targetFlat - currentPos
            if diff.Magnitude > 3 then
                hrp.Velocity = diff.Unit * 152 + Vector3.new(0, hrp.Velocity.Y, 0)
                hrp.CFrame = CFrame.lookAt(currentPos, targetFlat)
            else
                hrp.Velocity = Vector3.new(0, hrp.Velocity.Y, 0)
                hrp.CFrame = CFrame.new(targetFlat)
                isMoving = false
                if moveConn then moveConn:Disconnect() end
                stationBtn.Text = "Ø§Ù„Ù…Ø­Ø·Ø© Ø§Ù„ØªØ§Ù„ÙŠØ© ðŸƒâ€â™‚ï¸"
            end
        end)
    end
end)

local speedBtn = CreateBtn("Ø³Ø±Ø¹Ø© (152) âš¡", UDim2.new(0.075, 0, 0, 125))
local spdOn = false
speedBtn.Activated:Connect(function()
    spdOn = not spdOn
    speedBtn.BackgroundColor3 = spdOn and Color3.fromRGB(0, 255, 100) or Color3.fromRGB(255, 255, 255)
end)

rs.Heartbeat:Connect(function()
    local char = lp.Character
    local hum = char and char:FindFirstChildOfClass("Humanoid")
    local hrp = char and char:FindFirstChild("HumanoidRootPart")
    if spdOn and hrp and hum and hum.MoveDirection.Magnitude > 0 and not isMoving then
        hrp.Velocity = Vector3.new(hum.MoveDirection.X * 152, hrp.Velocity.Y, hum.MoveDirection.Z * 152)
    end
end)

-- Ø§Ù„Ù„Ù…Ø³ Ø§Ù„ØªÙ„Ù‚Ø§Ø¦ÙŠ
task.spawn(function()
    while true do
        task.wait(0.1)
        if lp.Character then
            for _, obj in pairs(workspace:GetDescendants()) do
                if obj:IsA("ProximityPrompt") then
                    obj.HoldDuration = 0
                    if (lp.Character:GetPivot().Position - obj.Parent:GetPivot().Position).Magnitude < 12 then
                        obj:InputHoldBegin() task.wait() obj:InputHoldEnd()
                    end
                end
            end
        end
    end
end)

-- Ù†Ø¬ÙˆÙ… Ø§Ù„Ø®Ù„ÙÙŠØ©
local function CreateStar()
    local star = Instance.new("Frame", main)
    star.BorderSizePixel = 0
    star.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
    star.Size = UDim2.new(0, math.random(1, 2), 0, math.random(1, 2))
    star.Position = UDim2.new(math.random(), 0, -0.1, 0)
    Instance.new("UICorner", star).CornerRadius = UDim.new(1, 0)
    local fallSpeed = math.random(15, 30) / 100
    task.spawn(function()
        while star and star.Parent do
            star.Position = star.Position + UDim2.new(0, 0, 0, fallSpeed)
            if star.Position.Y.Offset > main.AbsoluteSize.Y then star.Position = UDim2.new(math.random(), 0, -0.1, 0) end
            rs.RenderStepped:Wait()
        end
    end)
end
for i = 1, 30 do CreateStar() end
