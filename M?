-- Ù…Ø­Ø±Ùƒ Ø§Ù„Ù€ RGB (ØªÙ„ÙˆÙŠÙ† Ø§Ù„Ø¯Ø§Ø¦Ø±Ø© ÙˆØ§Ù„Ø¹Ù†ÙˆØ§Ù†)
task.spawn(function()
    local h = 0
    while true do
        h = h + 0.01
        local col = Color3.fromHSV(h, 0.8, 1)
        n60Title.TextColor3 = col
        open.TextColor3 = col
        stroke.Color = col
        task.wait(0.02)
        if h > 1 then h = 0 end
    end
end)

